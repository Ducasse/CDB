Class {
	#name : #CDBHashTest,
	#superclass : #TestCase,
	#category : #'CDBFormat-Test-New'
}

{ #category : #tests }
CDBHashTest >> testHash [
	"Note that these values were generated by another CDB implementation"
	
	| b1 b2 b3 b4 |
	b1 := #(0 128 255) asByteArray.
	b2 := #(0 0 0) asByteArray.
	b3 := #(15 33 57) asByteArray.
	b4 := #(255 255 255 255 255 255) asByteArray.
	self assert: b1 cdbHash equals: 193381274.
	self assert: b2 cdbHash equals: 193376997.
	self assert: b3 cdbHash equals: 193381394.
	self assert: b4 cdbHash equals: 1437972773
]

{ #category : #tests }
CDBHashTest >> testHash2 [

	self assert: #[1] cdbHash equals: 177572.
	self assert: #[11] cdbHash equals: 177582.
	self assert: #[1 1] cdbHash equals: 5859877.
]
